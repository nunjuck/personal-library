{
  "name": "library.koshelev.dev",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:no-watch": "react-scripts test --watchAll=false",
    "test:upgrade": "react-scripts test --watchAll=false -u",
    "eject": "react-scripts eject",
    "lint": "eslint src/ --ext .ts,.tsx --cache",
    "lint:fix": "yarn lint --fix",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "lint-staged": "lint-staged",
    "prepare": "husky install"
  },
  "dependencies": {
    "@notionhq/client": "^0.3.1",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/user-event": "^12.8.3",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.9.1",
    "@types/react": "^18.0.8",
    "@types/react-dom": "^18.0.3",
    "@types/react-lazy-load-image-component": "^1.5.2",
    "@types/react-router-dom": "^5.1.9",
    "axios": "^0.21.1",
    "http-proxy-middleware": "^2.0.1",
    "lint-staged": "^11.1.2",
    "normalize.css": "^8.0.1",
    "prettier": "2.6.2",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-lazy-load-image-component": "^1.5.1",
    "react-query": "^3.23.0",
    "react-router-dom": "^5.3.0",
    "react-scripts": "^5.0.1",
    "ts-jest": "^27.0.5",
    "web-vitals": "^1.1.2"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run test:changed",
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@testing-library/react": "^13.2.0",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "husky": "^7.0.2",
    "typescript": "^4.6.4"
  },
  "proxy": "https://api.notion.com/",
  "lint-staged": {
    "*.{ts,tsx}": [
      "yarn lint:fix",
      "yarn test --bail --watchAll=false --findRelatedTests --passWithNoTests"
    ],
    "*.{js,jsx,ts,tsx,css,md,json}": "yarn format"
  }
}
